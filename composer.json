{
    "name": "codefromhell/usaq-backend",
    "description": "Slim Framework 3 .",
    "license": "MIT",
    "type": "project",
    "keywords": ["slim-framework", "doctrine"],
    "authors": [
        {
            "name": "Ram√≥n Corchero",
            "email": "rjcorflo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "USaq\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\USaq\\": "tests/src"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "php-di/slim-bridge": "^1.0",
        "monolog/monolog": "^1.13",
        "league/fractal": "*",
        "respect/validation": "^1.0",
        "doctrine/orm": "^2.5",
        "vlucas/phpdotenv": "^2.4",
        "symfony/yaml": "^3.0",
        "consolidation/robo": "^1.0",
        "crell/api-problem": "^2.0",
        "gofabian/negotiation-middleware": "^0.1.2",
        "tuupola/cors-middleware": "^0.5.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "deployer/deployer": "^5.0",
        "herloct/codeception-slim-module": "^1.1"
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8000 -t public public/index.php",
        "ser": "robo dev:serve",
        "test": "robo dev:test",
        "abc": "php app/console.php dev:test",
        "fix": "php app/console.php dev:fix"
    }
}
